# Bashrc for my macmini

# Mac ls does not have color...
alias ls="`which ls`";
alias arc='ssh -Y nat@10.0.1.222'

# bash completion in osx, thanks to mac ports
if [ -f /opt/local/etc/bash_completion ]; then
    . /opt/local/etc/bash_completion
fi

# Colors
export CLICOLOR=1
export LSCOLORS=ExFxCxDxBxegedabagacad

[[ -s "/Users/nat/.rvm/scripts/rvm" ]] && source "/Users/nat/.rvm/scripts/rvm"  # This loads RVM into a shell session.

