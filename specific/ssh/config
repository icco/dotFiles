# A neat way to cache connections.
#ControlMaster auto
#ControlPath ~/.ssh/tmp/%h_%p_%r

Host *
  KeepAlive yes

Host *.corp.google.com
  Compression no
  Cipher blowfish-cbc
  User natwelch

Host abode
  Hostname abode.sfo.corp.google.com

Host work-tunnel
  Hostname arcplug.dls.corp.google.com
  DynamicForward 2001
  ExitOnForwardFailure yes
  ServerAliveInterval 5
  ServerAliveCountMax 3

Host arcplug
  Hostname arcplug.dls.corp.google.com

Host icco-me
  User nat
  Hostname 192.158.28.236
  IdentityFile ~/Dropbox/gce_key

Host icco.me
  User ubuntu
  IdentityFile ~/Projects/icco.me/conf/keys/ec2.pem

Host snowball
  Hostname snowball.icco.me
  Port 4022
  User nat
  Compression yes
  KeepAlive yes

Host athenaeum
  Hostname athenaeum.local
  User root

Host athenaeum-external
  User root
  ProxyCommand ssh -q snowball nc -q0 athenaeum.local 22

Host cabin
  Hostname cabin.icco.me
  User pi
  Compression yes
