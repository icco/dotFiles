#! /bin/zsh

case $1 in
tbp)
  git config user.email nat@timebyping.com
  git config user.signingkey F1977DF8
  git config commit.gpgsign true
  ;;
icco)
  git config user.email nat@natwelch.com
  git config user.signingkey 27E1EC35A2C3822379094DB5A931B605C4EEF77F
  git config commit.gpgsign true
  ;;
google)
  git config user.email natwelch@google.com
  git config commit.gpgsign false 
  ;;
*)
  echo "Current user $(git config --get user.email)"
  ;;
esac
